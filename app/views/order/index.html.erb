<h1 class="text-center">Orders</h1>

<% Order.first.items_ordered %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Price</th>
      <th scope="col">Items</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <th scope="row"><%= order.id %></th>
        <td><%= order.price %></td>
        <td><%= order.price %></td>
        <td><%= order.price %></td>

      </tr>
    <% end %>
  </tbody>
</table>